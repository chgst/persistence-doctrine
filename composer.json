{
    "name": "chgst/persistence-doctrine",
    "description": "Doctrine implementation for data store",
    "type": "library",
    "license": "MIT",
    "require": {
        "chgst/common":"^0.3.1",
        "doctrine/persistence": "^3.2.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^7.4.0 || ^8.0",
        "friends-of-phpspec/phpspec-code-coverage": "^6.3.0 || ^7.0",
        "php-coveralls/php-coveralls": "^2.6.0",
        "doctrine/orm": "^3.0",
        "doctrine/mongodb-odm": "^2.5.2",
        "dg/bypass-finals": "^1.5"
    },
    "config": {
        "platform": {
            "php": "8.2"
        }
    }
}
